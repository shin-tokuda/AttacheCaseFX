{
	"applications" : [
		{
			"app_id" : "sample",
			"title" : "サンプル",
			"icon" : "./image/16x16/edit.png",
			"extension" : "acfx0"
		},
		{
			"app_id" : "psnl001",
			"title" : "個人サンプル",
			"icon" : "./image/16x16/edit.png",
			"extension" : "acfx1"
		}
	],
	"drivers" : [
		{
			"type" : "DB2",
			"driver" : "com.ibm.db2.jcc.DB2Driver",
			"url" : "jdbc:db2://{server}[:{port}]/{databaseName}"
		},
		{
			"type" : "Oracle",
			"driver" : "oracle.jdbc.driver.OracleDriver",
			"url" : "jdbc:oracle:thin:@{server}[:{port}]:{SID}"
		},
		{
			"type" : "h2(Embeddedモード)",
			"driver" : "org.h2.Driver",
			"url" : "jdbc:h2:{databaseName}[;URLAttributes= [;...]]"
		},
		{
			"type" : "h2(TCP Serverモード)",
			"driver" : "org.h2.Driver",
			"url" : "jdbc:h2:tcp://{server}[:{port}]/{databaseName}[;URLAttributes= [;...]]"
		},
		{
			"type" : "Derby(Embeddedモード)",
			"driver" : "org.apache.derby.jdbc.EmbeddedDriver",
			"url" : "jdbc:derby:{databasePath}[;URLAttributes= [;...]]"
		},
		{
			"type" : "Derby(Client/Serverモード)",
			"driver" : "org.apache.derby.jdbc.ClientDriver",
			"url" : "jdbc:derby://{server}[:{port}]/{databaseName}[;URLAttributes= [;...]]"
		}
	]
}